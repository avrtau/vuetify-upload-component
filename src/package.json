{
  "name": "vuetify-upload-component",
  "version": "2.1.2",
  "main": "dist/vuetify-upload-component.umd.js",
  "module": "dist/vuetify-upload-component.esm.js",
  "unpkg": "dist/vuetify-upload-component.min.js",
  "browser": {
    "./sfc": "index.vue"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avrtau/vuetify-upload-component"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuetify-upload-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuetify-upload-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuetify-upload-component.min.js"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "description": "A simple upload component for Vuetify.",
  "bugs": "https://github.com/avrtau/vuetify_upload_component/issues",
  "license": "MIT",
  "author": {
    "name": "Avraham Tauberman",
    "url": "https://github.com/avrtau"
  },
  "keywords": [
    "vuetify",
    "upload"
  ]
}
